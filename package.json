{
    "name": "x86-64-assembly-pro",
    "displayName": "x86_64 Assembly Pro",
    "description": "Better syntax highlighting for x86_64 assembly",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.0.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "assembly",
                "aliases": [
                    "x86_64",
                    "assembly"
                ],
                "extensions": [
                    ".s",
                    ".asm",
                    ".S"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "assembly",
                "scopeName": "source.assembly.asm",
                "path": "./syntaxes/assembly.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "node": "^8.3.0"
    }
}
